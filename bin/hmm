import sys
from argparse import ArgumentParser, FileType

parser = ArgumentParser(description="POS Tagging using HMM")
parser.add_argument('-t', '--train', nargs='?', type=FileType('r'))
parser.add_argument('-T', '--test', nargs='?', type=FileType('r'))
parser.add_argument('-m', '--tag-map', nargs='?', type=FileType('r'))

args = parser.parse_args()


